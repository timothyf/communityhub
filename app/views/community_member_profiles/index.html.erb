<p id="notice"><%= notice %></p>

<h1>Community Member Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_member_profiles.each do |community_member_profile| %>
      <tr>
        <td><%= link_to 'Show', community_member_profile %></td>
        <td><%= link_to 'Edit', edit_community_member_profile_path(community_member_profile) %></td>
        <td><%= link_to 'Destroy', community_member_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community Member Profile', new_community_member_profile_path %>
