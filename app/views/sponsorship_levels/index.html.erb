<p id="notice"><%= notice %></p>

<h1>Sponsorship Levels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsorship_levels.each do |sponsorship_level| %>
      <tr>
        <td><%= link_to 'Show', sponsorship_level %></td>
        <td><%= link_to 'Edit', edit_sponsorship_level_path(sponsorship_level) %></td>
        <td><%= link_to 'Destroy', sponsorship_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sponsorship Level', new_sponsorship_level_path %>
